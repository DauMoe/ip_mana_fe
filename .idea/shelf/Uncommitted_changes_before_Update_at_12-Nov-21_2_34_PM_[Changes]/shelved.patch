rol\r\n    width: calc(100% - 20px)\r\n    min-width: 350px\r\n    min-height: 30px\r\n    padding: 3px 10px\r\n    border-radius: 6px\r\n    outline: none\r\n    border: 1px solid black\r\n\r\n.hide\r\n    display: none\r\n\r\n.show\r\n    display: block\r\n\r\n.bold\r\n    font-weight: bold\r\n\r\n.theme_green\r\n    background-color: $theme_green !important\r\n    color: $white !important\r\n\r\n.theme_brown\r\n    background-color: $theme_brown !important\r\n    color: $white !important\r\n\r\n.btn\r\n    padding: 10px 20px\r\n    border: none\r\n    border-radius: 6px\r\n    background-color: #cacaca\r\n    color: black\r\n    cursor: pointer\r\n\r\n.pull-right\r\n    float: right\r\n\r\n.text-center\r\n    text-align: center\r\n\r\n.text-right\r\n    text-align: right\r\n\r\n.nice_theme\r\n    //width: calc(100% - 40px)\r\n    width: 100%\r\n    thead\r\n        text-align: center\r\n        td\r\n            font-weight: bold\r\n            padding: 10px 0\r\n            color: $white\r\n            background-color: $theme_brown\r\n        td:first-child\r\n            border-radius: 6px 0 0 0\r\n        td:last-child\r\n            border-radius: 0 6px 0 0\r\n    tbody\r\n        tr\r\n            td\r\n                padding: 13px 10px\r\n        tr:nth-child(even)\r\n            background-color: #dddddd
